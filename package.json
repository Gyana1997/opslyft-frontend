    "testMatch": [
      "<rootDir>/__tests__/*.test.js"
    ],
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
